        "background_color": "#2a3670"
    },
    "features": {
        "bot_user": {
            "display_name": "Hack Hour Dev",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/testhack",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
                "description": "Start a New Session",
                "should_escape": false
            },
            {
                "command": "/testcancel",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
                "description": "Cancel Current Session",
                "should_escape": false
            },
            {
                "command": "/testpause",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
                "description": "Pause your current hour",
                "should_escape": false
            },
            {
                "command": "/teststart",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
                "description": "Continue the hour",
                "should_escape": false
            },
            {
                "command": "/testextend",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
                "description": "Extends the hour",
                "should_escape": false
            },
            {
                "command": "/teststats",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
                "description": "View your stats",
                "should_escape": false
            },
            {
                "command": "/testsessions",
                "url": "https://optimum-stork-daily.ngrok-free.app/slack/events",
